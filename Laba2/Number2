package Laba2;
import java.io.File;
public class Number2 {
    public static void main(String[] args) {
        try {
            File file = new File("basedir\\Picture");
            if ( !file.exists()){
                if (file.mkdir())
                    System.out.println("Создана директория Picture");
            }
            file = new File("basedir\\Text");
            if ( !file.exists()){
                if (file.mkdir())
                    System.out.println("Создана директория Picture");
            }
            file = new File("basedir\\Text\\History");
            if ( !file.exists()){
                if (file.mkdir())
                    System.out.println("Создана директория Picture");
            }
        }
        catch(Exception e){
            System.out.println("Error");
        }
    }
}
